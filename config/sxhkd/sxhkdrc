mod4 + F5
	~/.sh/changewallpaper
mod4 + w
	$BROWSER
mod4 + shift + w
	$TERMINAL -e nmtui
mod4 + n
	$TERMINAL -e ranger
mod4 + m
	$TERMINAL -e cmus
mod4 + shift + m
	$TERMINAL -e pulsemixer
mod4 + b
	$TERMINAL -e neomutt
XF86TouchpadToggle
	~/.sh/touchpad
XF86MonBrightnessUp
	light -A 10
XF86MonBrightnessDown
	light -U 10
XF86AudioMute
	pamixer -t
XF86AudioRaiseVolume
	pamixer --allow-boost -i 5
XF86AudioLowerVolume
	pamixer --allow-boost -d 5
XF86Sleep
	sudo -A zzz
XF86AudioPrev
	cmus-remote --prev
XF86AudioNext
	cmus-remote --next
XF86AudioPlay
	cmus-remote --pause
XF86AudioStop
	cmus-remote --stop
XF86Display
	arandr
XF86Launch1
	$TERMINAL -e ssh root@nagbalae.hu
mod4 + Print
	.sh/screenshot
mod4 + a
	passmenu
